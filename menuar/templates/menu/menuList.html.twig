
<div class="accordion" id="accordionExample">
    {% for key,value in menuItems %}
        <div class="card" style="margin-bottom: 10px; border-radius: 12px;">
        <div class="card-header" id="headingOne">
            <h2 class="mb-0">
                <button class="btn btn-link btn-block text-left" type="button" data-toggle="collapse" data-target="{{'#' ~ key }}" aria-expanded="true" aria-controls="collapseOne">
                    {{ key }}
                </button>
            </h2>
            <div id="{{ key }}" class="collapse show" aria-labelledby="headingOne" data-parent="#accordionExample">
                <div class="album py-5 bg-light">
                    {% include 'menu/menuListGroup.html.twig' with value %}
                </div>
            </div>
        </div>
    </div>
    {% endfor %}
</div>